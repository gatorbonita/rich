# Environment Configuration Example
# Copy this file to .env and adjust values as needed

# Application
APP_ENV=development
APP_DEBUG=true

# Cache
CACHE_DIR=./cache
CACHE_TTL_HOURS=24

# Data Windows (in months)
RETURN_WINDOW_MONTHS=3
VOLATILITY_WINDOW_MONTHS=12
CORRELATION_WINDOW_MONTHS=12

# Optimization
MAX_OPTIMIZATION_TIME_SECONDS=2
N_ITERATIONS=1000
TOP_K_PORTFOLIOS=3

# API
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
